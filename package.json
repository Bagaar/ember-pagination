    "ember-resolver": "^5.0.1",
    "ember-source": "~3.4.0",
    "ember-source-channel-url": "^1.1.0",
    "ember-try": "^1.0.0",
    "eslint-plugin-ember": "^6.2.0",
    "eslint-plugin-node": "^8.0.1",
    "husky": "^1.3.1",
