<PaginationData
  @currentPage={{5}}
  @itemsPerPage={{10}}
  @pageMargins={{2}}
  @pageRange={{3}}
  @totalItems={{90}}
  as |data|
>
  <dl>
    <dt>activeItems</dt>
    <dd>{{data.activeItems}}</dd>

    <dt>allPages</dt>
    <dd>
      {{#each data.allPages as |page|}}
        {{page}}
      {{/each}}
    </dd>

    <dt>currentPage</dt>
    <dd>{{data.currentPage}}</dd>

    <dt>endMarginPages</dt>
    <dd>
      {{#each data.endMarginPages as |page|}}
        {{page}}
      {{/each}}
    </dd>

    <dt>firstActiveItem</dt>
    <dd>{{data.firstActiveItem}}</dd>

    <dt>isFirstPage</dt>
    <dd>{{data.isFirstPage}}</dd>

    <dt>isLastPage</dt>
    <dd>{{data.isLastPage}}</dd>

    <dt>itemsPerPage</dt>
    <dd>{{data.itemsPerPage}}</dd>

    <dt>lastActiveItem</dt>
    <dd>{{data.lastActiveItem}}</dd>

    <dt>lastPage</dt>
    <dd>{{data.lastPage}}</dd>

    <dt>nextPage</dt>
    <dd>{{data.nextPage}}</dd>

    <dt>pageMargins</dt>
    <dd>{{data.pageMargins}}</dd>

    <dt>pageRange</dt>
    <dd>{{data.pageRange}}</dd>

    <dt>pageRangePages</dt>
    <dd>
      {{#each data.pageRangePages as |page|}}
        {{page}}
      {{/each}}
    </dd>

    <dt>previousPage</dt>
    <dd>{{data.previousPage}}</dd>

    <dt>shouldShowLowerBreak</dt>
    <dd>{{data.shouldShowLowerBreak}}</dd>

    <dt>shouldShowUpperBreak</dt>
    <dd>{{data.shouldShowUpperBreak}}</dd>

    <dt>startMarginPages</dt>
    <dd>
      {{#each data.startMarginPages as |page|}}
        {{page}}
      {{/each}}
    </dd>

    <dt>totalItems</dt>
    <dd>{{data.totalItems}}</dd>

    <dt>totalPages</dt>
    <dd>{{data.totalPages}}</dd>
  </dl>
</PaginationData>