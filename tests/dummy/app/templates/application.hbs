<PaginationData
  @currentPage={{5}}
  @itemsPerPage={{10}}
  @pageMargins={{2}}
  @pageRange={{3}}
  @totalItems={{90}}
  as |data|
>
  <dl>
    <dt>activeItems</dt>
    <dd>{{data.activeItems}}</dd>

    <dt>allPages</dt>
    <dd>{{data.allPages}}</dd>

    <dt>currentPage</dt>
    <dd>{{data.currentPage}}</dd>

    <dt>endMarginPages</dt>
    <dd>{{data.endMarginPages}}</dd>

    <dt>isFirstPage</dt>
    <dd>{{data.isFirstPage}}</dd>

    <dt>isLastPage</dt>
    <dd>{{data.isLastPage}}</dd>

    <dt>itemsPerPage</dt>
    <dd>{{data.itemsPerPage}}</dd>

    <dt>lastPage</dt>
    <dd>{{data.lastPage}}</dd>

    <dt>nextPage</dt>
    <dd>{{data.nextPage}}</dd>

    <dt>pageRangePages</dt>
    <dd>{{data.pageRangePages}}</dd>

    <dt>previousPage</dt>
    <dd>{{data.previousPage}}</dd>

    <dt>shouldShowLowerBreak</dt>
    <dd>{{data.shouldShowLowerBreak}}</dd>

    <dt>shouldShowUpperBreak</dt>
    <dd>{{data.shouldShowUpperBreak}}</dd>

    <dt>startMarginPages</dt>
    <dd>{{data.startMarginPages}}</dd>

    <dt>totalPages</dt>
    <dd>{{data.totalPages}}</dd>
  </dl>
</PaginationData>